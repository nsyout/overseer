{
  "name": "@overseer/ui",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "concurrently -n api,vite -c blue,green \"tsx watch src/api/index.ts\" \"vite\"",
    "dev:api": "tsx watch src/api/index.ts",
    "dev:vite": "vite",
    "build": "vite build",
    "build:full": "vite build && tsc --project tsconfig.node.json",
    "build:server": "node scripts/build-server.mjs",
    "typecheck": "tsc --noEmit",
    "test:ui": "./scripts/test-ui.sh test",
    "test:ui:quick": "./scripts/test-ui.sh quick",
    "test:ui:snapshot": "./scripts/test-ui.sh snapshot",
    "test:ui:screenshot": "./scripts/test-ui.sh screenshot",
    "test:ui:watch": "./scripts/test-ui.sh watch",
    "test:ui:interact": "./scripts/test-ui.sh interact",
    "test:ui:verify": "./scripts/ai-review.sh verify",
    "test:ui:capture": "./scripts/ai-review.sh capture",
    "test:ui:flow": "./scripts/ai-review.sh flow"
  },
  "dependencies": {
    "@dagrejs/dagre": "^1.1.8",
    "@hono/node-server": "^1.19.9",
    "@tanstack/react-query": "^5.66.0",
    "@xyflow/react": "^12.10.0",
    "better-result": "^2.7.0",
    "hono": "^4.11.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "esbuild": "^0.24.2",
    "@tailwindcss/vite": "^4.0.6",
    "@types/node": "^22.10.10",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "tailwindcss": "^4.0.6",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vite": "^6.0.11"
  }
}
